generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:../data/database.db"
}

model User {
  id         Int      @id @default(autoincrement())
  telegramID BigInt   @unique
  createdAt  DateTime @default(now())
  context    Json
}

model Program {
  id                 Int  @id @default(autoincrement())
  context            Json
  operationLabelList Json
  current            Int
}
